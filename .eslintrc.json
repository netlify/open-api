{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:n/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "import/order": [2, { "newlines-between": "always" }]
  }
}
